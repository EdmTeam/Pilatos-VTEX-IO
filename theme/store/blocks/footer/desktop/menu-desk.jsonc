{
    "footer-layout.desktop": {
        "title": "Footer DESK",
        "children": [
            "flex-layout.row#desk-menu-item",
            "flex-layout.row#legal-medios-pago-desk"
        ]
    },

    "sandbox#footer": {
        "props": {
          "width": "200px",
          "height": "60px",
          "initialContent": "<script src='https://bluebird-web-agent.s3.us-east-2.amazonaws.com/keybe.js'></script><script>window.addEventListener('load', function () {var configChat = { apiKey: '58a0b387630c43da91670cb9ea94fef1' } window.keybe.webchatConversationsUiLoad(configChat) }) </script>",
          "allowCookies": true
        }
    },

    "flex-layout.row#desk-menu-item": {
      "title": "Footer menu DESK",
        "children": [
     
        
            "flex-layout.col#news-letter-desk",
            "flex-layout.col#desk-menu-item-col-1",
            "flex-layout.col#desk-menu-item-col-2",
            "flex-layout.col#desk-menu-item-col-3",
            "flex-layout.col#desk-menu-item-col-4",
            "flex-layout.col#desk-menu-item-col-5"
        ],
        "props": {
            "blockClass": "contentFooter"
        }
    },

    "flex-layout.col#news-letter-desk":{
        "children":[
            "flex-layout.col#news-letter-row-1"
        ],
        "props":{
            "blockClass": ["contentNewsletter", "contentNewsletterMobile"]
        }
    },

