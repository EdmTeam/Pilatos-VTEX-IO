        "props": {
            "text": "",
            "blockClass": "icoBlacksip"
        }
    },

    "flex-layout.col#legal-ico-vtex":{
        "children":[
            "rich-text#legal-ico-vtex"
        ],
        "props":{
            "width": "8%"
        }
    },

    "rich-text#legal-ico-vtex":{
        "props": {
            "text": "",
