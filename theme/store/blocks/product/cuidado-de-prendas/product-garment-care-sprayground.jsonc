    "disclosure-trigger#product-content-garment-care-sprayground": {
        "children": [
            "rich-text#garment-care-sprayground",
            "disclosure-state-indicator#garment-care"
        ],
        "props": {
            "blockClass": ["container-products", "disclosure-indicator-garment-care"]
        }
    },

    "rich-text#garment-care-sprayground": {
        "props": {
            "text": "**Cuidados de la prenda**"
            //"blockClass": "product-description-pdp"
        }
    },
    "disclosure-content#product-description-garment-care-sprayground": {
        "children": [
            "rich-text#garment-care-use-sprayground",
            "flex-layout.row#garment-use-sprayground"
        ],
        "props": {
            "blockClass": "garment-use-product-pdp"
        }
    },
    
    "rich-text#garment-care-use-sprayground": {
        "props": {
            "text": "**Recomendaciones de uso**"
        }
    },
