{
    "condition-layout.product#garment-care-sprayground": {
        "props": {
            "conditions": [
                {
                    "subject": "brandId",
                    "arguments": {
                        "id": "2000121"
                    }
                }
            ],
            "Then": "disclosure-layout#product-garment-care-sprayground"
        }
    },

    "disclosure-layout#product-garment-care-sprayground": {
        "children": [
            "disclosure-trigger#product-content-garment-care-sprayground",
            "disclosure-content#product-description-garment-care-sprayground"
        ]
