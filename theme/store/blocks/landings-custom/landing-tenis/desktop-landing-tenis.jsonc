    "info-card#banner-hero-tenis-desk": {
        "props": {
            "id": "info-card-example",
            "isFullModeStyle": true,
            "textPosition": "left",
            "imageUrl": "/arquivos/Pilatos-LandingTenis-BannerPpal-Desk.jpg",
            "mobileImageUrl": "/arquivos/Pilatos-LandingTenis-BannerPpal-Mob.jpg",
            "headline": "Sneakers",
