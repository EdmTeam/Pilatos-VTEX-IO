            "image#landingTop-cat-4"
        ],
        "props":{
            "width": "50%"
        }
    },

    "image#landingTop-cat-4": {
        "props": {
            "src": "/arquivos/linea-buzosychaquetas-pc.jpg",
            "link": {
                "url": "/1296?map=productClusterIds&order=OrderByTopSaleDESC",
                "attributeTitle": ""
            },
            "blockClass": "departmentBannerImg-top"
        }
