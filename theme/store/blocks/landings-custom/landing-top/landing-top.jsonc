{
    "store.custom#landingTop":{
        "blocks":[
            "rich-text#landing-marcas-bread-crumb",
           "flex-layout.row#bannerLandingTop",
           
            //"flex-layout.row#landingTop-recomendador",
            "search-result-layout.customQuery#landing-productosTop",
            "flex-layout.row#banner-landingTop-footer"
        ],
        "props":{
            "blockClass": "contentStoreCustom"
        }
    },
    "flex-layout.row#bannerLandingTop":{
        "children":[
            "flex-layout.col#banner-landingTop"
        ]
    },

    "flex-layout.col#banner-landingTop":{
        "children":[
           "responsive-layout.desktop#banner-landingTop",
            "responsive-layout.mobile#banner-landingTop"
             
          ]
    },

    "responsive-layout.desktop#banner-landingTop":{
        "children":[
            "flex-layout.col#banner-landingTop-principal",
            "rich-text#landing-marcas-premium",
            "flex-layout.row#banner-landingTop-imgList"
        ]
    },

    "rich-text#landing-marcas-premium":{
        "props":{
            "text": "**Productos Top en Pilatos 2022**",
            "blockClass": "tittleBannersMarcasTop",
            "textAlignment": "CENTER",
            "textPosition": "CENTER"
        }
    },

    "flex-layout.col#banner-landingTop-principal":{
        "children":[
            "image#banner-landingTop-principal"
        ],
        "props":{
            "blockClass":"contentBannersDesk"
        }
    },

    "image#banner-landingTop-principal": {
        "props": {
            "src": "/arquivos/Pilatos-Landing-linea-LANDING-pc.jpg",
            "link": {
                "url": "",
                "attributeTitle": ""
            },
            "blockClass": "imageBanner"
        }
    },

    "flex-layout.row#banner-landingTop-imgList":{
        "children":[
            "flex-layout.col#landingTop-cat-1",
            "flex-layout.col#landingTop-cat-2",
            "flex-layout.col#landingTop-cat-3",
            "flex-layout.col#landingTop-cat-4",
            "flex-layout.col#landingTop-cat-5"
        ],
        "props":{
            "blockClass":"contentBanners-2"
        }
    },

    "flex-layout.col#landingTop-cat-1":{
        "children":[
            "image#landingTop-cat-1"
        ],
        "props":{
            "width": "50%"
        }
    },

    "image#landingTop-cat-1": {
        "props": {
            "src": "/arquivos/linea-jeans-pc.jpg",
            "link": {
                "url": "/1292?map=productClusterIds&order=OrderByTopSaleDESC",
                "attributeTitle": ""
            },
            "blockClass": "departmentBannerImg-top"
        }
    },

    "flex-layout.col#landingTop-cat-2":{
        "children":[
            "image#landingTop-cat-2"
        ],
        "props":{
