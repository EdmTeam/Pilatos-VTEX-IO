            "link": {
                "url": "/1293?map=productClusterIds&order=OrderByTopSaleDESC",
                "attributeTitle": ""
            },
            "blockClass": "departmentBannerImg-top"
        }
    },

    "flex-layout.col#landingTop-cat-3":{
        "children":[
            "image#landingTop-cat-3"
        ],
        "props":{
            "width": "50%"
        }
    },

    "image#landingTop-cat-3": {
        "props": {
