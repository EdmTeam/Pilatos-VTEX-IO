    "search-result-layout.customQuery#landing-productosTop": {
        "blocks": [
            "search-result-layout.desktop",
            "search-result-layout.mobile#dosbanner"
        ],
        "props": {
            "showProductsCount": true,
            "querySchema": {
                "orderByField": "OrderByScoreDESC",
                "hideUnavailableItems": true,
                "maxItemsPerPage": 48,
                "queryField": "1291",
                "mapField": "productClusterIds"
            }
        }
    },

    // bloque inferior con cambios y devoluciones

    "flex-layout.row#banner-landingTop-footer":{
        "children":[
            "flex-layout.col#landingTop-cambiosydevoluciones",
            "flex-layout.col#landingTop-envios"
        ],
        "props":{
            "blockClass":"contentBanners-2"
        }
    },

