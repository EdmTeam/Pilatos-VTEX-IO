        }
      ]
    }
  },

  "slider-layout#banner-landing-nuevas-llegadas": {
    "title": "Productos",
    "props": {
      "itemsPerPage": {
        "desktop": 30,
        "tablet": 30,
        "phone": 30
      },
      "showNavigationArrows": "desktopOnly",
      "showPaginationDots": "never",
      "infinite": false,
      "fullWidth": true,
      "blockClass": "slide__banner"
    }
  },
