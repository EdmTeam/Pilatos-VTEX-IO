      "slider-layout#banner-landing-nuevas-llegadas"
    ],
    "props": {
      "height": 570,
      "preload": true,
      "images": [
        {
          "image": "https://dieselcolombia.vtexassets.com/assets/vtex.file-manager-graphql/images/546f5d85-b623-4eda-ad33-f1347b127d69___d92778d4dfbcb317247b1599a32f3d3c.gif",
