{
    "store.custom#landing-memorama":{
        "blocks":[
            "flex-layout.row#banner-landing-memorama",
            // "memorygame",
            "rich-text#landing-memorama-text-seo-h1",
            "search-result-layout.customQuery#landing-memorama",
            "responsive-layout.desktop#landing-banner-video-banners-memorama",
            "responsive-layout.phone#landing-banner-video-banners-memorama",
            "responsive-layout.tablet#landing-banner-video-banners-memorama"
       /*      "flex-layout.row#banner-landing-memorama-piedepagina",
            "responsive-layout.phone#banner-landing-memorama-piedepagina-phone",
            "responsive-layout.tablet#banner-landing-memorama-piedepagina-tablet" */
        ],
        "props":{
            "blockClass": "contentStoreCustom"
        }
    },



    "rich-text#landing-memorama-text-seo-h1": {
        "props": {
            "textAlignment": "CENTER",
            "textPosition": "CENTER",
            "text": "### Compra nuestra esencia",
            "blockClass": "texto-memorama-textseo-h1"
        }
    },


    "search-result-layout.customQuery#landing-memorama": {
        "blocks": [
            "search-result-layout.desktop#landing-memorama",
            "search-result-layout.mobile#landing-memorama"
        ],
        "props": {
            "querySchema": {
                "queryField": "1228",
                "mapField": "productClusterIds"
            }
        }
    },
    "search-result-layout.desktop#landing-memorama": {
        "children": [
            "flex-layout.row#result-landing-memorama",
            "flex-layout.row#spacer"
        ],
        "props": {
            "preventRouteChange": true,
            "hideUnavailableItems": true,
            "showFacetQuantity": true,
            "mobileLayout": {
                "mode1": "small",
                "mode2": "normal"
            },
            "__unstableProductOriginVtex": true
        }
    },
    "flex-layout.row#result-landing-memorama": {
        "children": [
          "flex-layout.col#content"
        ],
        "props": {
          "preventHorizontalStretch": true,
          "fullWidth": true,
          "blockClass":"contentResultFilterOrderFromChaos"
        }
      },
    "search-result-layout.mobile#landing-memorama": {
        "children": [
            "flex-layout.row#bread-crumb",
            "search-title.v2",
            "flex-layout.row#search-info-mobile",
            "flex-layout.row#fetch-previous",
            "flex-layout.row#content-mobile",
            "flex-layout.row#fetch-more",
            "flex-layout.row#spacer"
        ],
        "props": {
            "hiddenFacets": {
                "specificationFilters": {
                    "hiddenFilters": [
                        {
                            "name": "Brand"
                        },
                        {
                            "name": "Department"
                        },
                        {
                            "name": "Category"
                        },
                        {
                            "name": "Subcategory"
                        },
                        {
                            "name": "Material"
                        },
                        {
                            "name": "Alto (Cm)"
                        },
                        {
                            "name": "Ancho (Cm)"
                        },
                        {
                            "name": "Breve Descripción"
                        },
                        {
                            "name": "Descripción"
                        },
                        {
                            "name": "Material"
                        }
                    ]
                }
            },
            "__unstableProductOriginVtex": true,
            "mobileLayout": {
                "mode1": "small",
                "mode2": "normal"
            }
        }
    },


    "flex-layout.row#banner-landing-memorama":{
        "children":[
            "flex-layout.col#banner-landing-memorama"
        ],
        "props":{
            "blockClass": "contentSearch"
        }
    },

