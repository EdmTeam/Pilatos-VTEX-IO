    "flex-layout.row#carruselProductosFinalAlianza":{
        "children": [
           "flex-layout.col#most-CarruselProductosFinalAlianza"
          ],
          "props": {
              "blockClass": "sliderContentShelf"
          }
    },
    "flex-layout.col#most-CarruselProductosFinalAlianza":{
        "children": [
            "vtex.product-summary:list-context.product-list#carruselProductosFinalAlianza"
        ]
    },
    "vtex.product-summary:list-context.product-list#carruselProductosFinalAlianza":{
        "blocks": [
            "vtex.product-summary:product-summary.shelf#most-viewed"
          ],
          "children": [
            "slider-layout#productosCarruselAlianzaFinal"
          ],
          "props": {
              "orderBy": "OrderByTopSaleDESC",
              "collection": "1291",
              "maxItems": 30
          }
    },
    "slider-layout#productosCarruselAlianzaFinal":{
        "props": {
            "itemsPerPage": {
              "desktop": 4,
              "tablet": 3,
              "phone": 2
            },
            "infinite": true,
            "showNavigationArrows": "desktopOnly",
            "showPaginationDots": "always"
         
          }
    }
}