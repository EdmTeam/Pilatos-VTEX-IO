            "responsive-layout.phone#BannerLandingSanValentin"
        ],
        "props": {
            "blockClass": "contentStoreCustom"
        }
    },
    "responsive-layout.desktop#BannerLandingSanValentin": {
        "children": [
            
            "flex-layout.row#BannerLandingSanValentin",
            "flex-layout.row#SanValentinPrincipal",
            "flex-layout.row#textoSanValentin",
            "flex-layout.row#botonSanValentin",
            "flex-layout.row#regaloPerfectoHombre",
            "flex-layout.row#regaloPerfectoHombreCat",
            "flex-layout.row#regaloPerfectoMujer",
