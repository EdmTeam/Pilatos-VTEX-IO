            "flex-layout.row#most-viewed",
            "flex-layout.row#textoSeoValentin"

        ],
        "props": {
            "blockClass": "contentSearch"
        }
    },

    "responsive-layout.tablet#BannerLandingSanValentin": {
        "children": [
            
            "flex-layout.row#BannerLandingSanValentin",
            "flex-layout.row#SanValentinPrincipal",
            "flex-layout.row#textoSanValentin",
