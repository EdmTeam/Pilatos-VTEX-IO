        "rich-text#sanValentinTitle"

    ]
},
  
"rich-text#sanValentinTitle": {
    "props": {
        "text": "# Pilatos San Valentin",
        "textAlignment": "LEFT",
        "blockClass": "departmentTitle"

    }

},

