        ],
        "props": {
            "blockClass": "contentBotones"
        }
    },


    "rich-text#sanValentin-texto": {
        "props": {
            "textAlignment": "CENTER",
            "textPosition": "CENTER",
            "text": "### ¿Buscas el regalo perfecto? \n Encuéntralo en nuestra **#GuíaPilatos** y sorprende en San Valentín",
