    "rich-text#title-best-sneaker":{
        "props":{
            "text": "Premium ",
            "blockClass": "sneaker-title-vitrina"
        }
    },

    "rich-text#title-best-sneaker-dos":{
      "props":{
          "text": "Â¡Destaca con estilo! camina la exclusividad en cada paso con nuestros tenis exclusivos ",
          "blockClass": "sneaker-title-vitrina-dos"
      }
  },

    "vtex.product-summary:list-context.product-list#sneaker-recomendador": {
        "blocks": [
            "vtex.product-summary:product-summary.shelf#most-viewed"
        ],
        "children": [
            "slider-layout#home-shelf-sneaker-recomendadoree"
        ],
        "props": {
            "orderBy": "OrderByTopSaleDESC",
            "collection": "1539",
            "maxItems": 30
        }
    },
    "slider-layout#home-shelf-sneaker-recomendadoree": {
        "props": {
          "itemsPerPage": {
            "desktop": 4,
            "tablet": 3,
            "phone": 2
          },
          "infinite": true,
          "showNavigationArrows": "desktopOnly",
          "showPaginationDots": "always"
        //   "centerMode": {
        //     "mobile": "to-the-left",
        //     "desktop": "disabled"
        //   }
        }
    },

