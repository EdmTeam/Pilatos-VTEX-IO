        "src": "/arquivos/monastery-gorras-desk-junio-2023.jpg",
        "blockClass":"estilosCategoriasMonastery",
        "link": {
            "url": "/monastery-gorras"
        },
        "maxWidth":"300px"   
    }
},
"image#categoriaPantalonetas-tablet":{
    "props":{
        "src": "/arquivos/monastery-pantalonetas-desk-junio-2023.jpg",
