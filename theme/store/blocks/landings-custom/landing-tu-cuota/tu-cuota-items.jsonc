        "children": [
            "rich-text#paso-a",
            "rich-text#paso-b",
            "rich-text#paso-c",
            "image#aceptar-captcha-img"
        ],
        "props": {
            "rowGap": 4,
            "blockClass": "trigger-content-informativo"
        }
