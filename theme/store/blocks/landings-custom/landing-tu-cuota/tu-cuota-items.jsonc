            "disclosure-trigger#captcha-trigger-title-content",
            "disclosure-content#captcha-trigger-info"
        ]
    },
    "disclosure-trigger#captcha-trigger-title-content": {
        "title": "Contenido titulo 1 paso",
        "children": [
            "rich-text#captcha-title",
            "disclosure-state-indicator"
        ],
        "props": {
            "blockClass": "trigger-content-icon",
