    "flex-layout.row#title-and-form-dsi": {
        "children": [
            "flex-layout.col#title-dsi",
            "flex-layout.col#form-dsi"
        ]
    },
    "flex-layout.col#title-dsi": {
        "children": [
            "rich-text#title-dsi",
            "rich-text#subtitle-dsi"
        ]
    },
    "rich-text#title-dsi": {
        "props": {
            "textAlignment": "CENTER",
            "text": "# DÍA SIN IVA:\nREGÍSTRATE",
            "font": "t-heading-1",
            "blockClass": "title-dsi"
        }
    },
    "rich-text#subtitle-dsi": {
        "props": {
            "textAlignment": "RIGHT",
            "text": "*Inscríbete aquí para ser el primero en enterarte de todos los detalles del Día sin IVA.*",
            "font": "t-body",
            "blockClass": "subtitle-dsi"
        }
    },
