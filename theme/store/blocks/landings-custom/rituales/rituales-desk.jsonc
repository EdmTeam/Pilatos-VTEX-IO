"flex-layout.col#seoFinalRituales":{
  "children": [
    "rich-text#ritualesSeoLargo",
    "rich-text#btonRitualesFinal"
  ]
},


  "rich-text#ritualesSeoLargo": {
    "title": "texto seo largo",
    "props": {
      "text": "Somos PILATOS MARCA, creemos en el poder de la moda y en la energía transformadora de los beats. Este año, celebramos en el rincón del mundo donde se vive la fiesta más grande la ciudad. Somos creyentes de que ser cool y arriesgado no es suficiente, tienes que transmitirlo a través de tus prendas y para eso estamos nosotros.",
      "textPosition": "LEFT",
      "blockClass": "ritualesSeoLargo"
    }
  },
  "rich-text#btonRitualesFinal":{
    "props": {
      "text": "[Ver colección](/rituales-productos)",
      "textAlignment": "LEFT",
      "textPosition": "CENTER",
      "blockClass": "btnPrincipal"
    }
  }


}
