    "list-context.image-list#spotifyDeskImage"
  ],
  "props": {
    "blockClass": "spotifyImage"

  }
},

"list-context.image-list#spotifyDeskImage": {
  "children": [
      "slider-layout"
  ],
  "props": {
      "preload": true,
      "images": [
          {
