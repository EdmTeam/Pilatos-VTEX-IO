            "image#landing-pilatos-marca-banners-1-img-1-mobile"
        ],
        "props":{
            "blockClass":"contentBannersDesk"
        }
    },

    "image#landing-pilatos-marca-banners-1-img-1-mobile": {
        "props": {
            "src": "/arquivos/Pilatos-RitvalesLanding-Mobile.jpg",
            "link": {
                "url": "",
                "attributeTitle": ""
            },
            "blockClass": "departmentBannerImg-1"
        }
    },

    "search-result-layout.mobile#landing-pilatos-marca": {
        "children": [
          "flex-layout.row#banner-search-mobile",
          "flex-layout.row#bread-crumb",
          "flex-layout.row#suggestion",
          //"flex-layout.row#most-viewed",
          "flex-layout.row#most-viewed-landing-pilatos",
          "responsive-layout.phone#landing-pilatos-marca-banners-2-phone",
          "responsive-layout.tablet#landing-pilatos-marca-banners-2-tablet",
          //"flex-layout.row#brands",
          //"flex-layout.row#product-count-mobile",
          "flex-layout.row#search-info-mobile-landing-pilatos-marca",
          "flex-layout.row#info-sec-mobile",
          "flex-layout.row#fetch-previous",
          "flex-layout.row#result-pilatos",
          //"flex-layout.row#content-mobile",
          "flex-layout.row#fetch-more"
          

        ],
        "props": {
            "showProductsCount": true,
            "blockClass": "searchResultMobile",
            "preventRouteChange": true,
            "hiddenFacets": {
                "specificationFilters": {
                    "hiddenFilters": [
                       {
                        "name": "Brand"
                        },
                        {
                        "name": "Department"
                        },
                        {
                        "name": "Category"
                        },
                        {
                        "name": "Subcategory"
                        },
                        {
                        "name": "Material"
                        },
                        {
                        "name": "Alto (Cm)"
                        },
                        {
                        "name": "Ancho (Cm)"
                        },
                        {
                        "name": "Breve Descripción"
                        },
                        {
                        "name": "Descripción"
                        },
                        {
                        "name": "Material"
                        },
                        {
                          "name": "Color"
                        }, 
                        {
                            "name": "Categoría"
                      }
                    ]
                }
            },
            "__unstableProductOriginVtex": true,
            "mobileLayout": {
                "mode1": "small",
                "mode2": "normal"
            }
        }
    },

    "flex-layout.row#search-info-mobile-landing-pilatos-marca": {
        "children": [
          "flex-layout.col#filterMobile",
          "flex-layout.col#order-by-mobile"
        ],
        "props": {
          "preserveLayoutOnMobile": true,
          "colSizing": "auto",
          "colJustify": "around",
          "blockClass": "search-in-mobile"
        }
    },

    "responsive-layout.phone#landing-pilatos-marca-banners-2-phone":{
