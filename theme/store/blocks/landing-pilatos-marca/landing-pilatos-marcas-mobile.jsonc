{
    "responsive-layout.phone#landing-pilatos-marca-banners":{
        "children":[
            "flex-layout.col#landing-pilatos-marca-banners-1-mobile"
        ]
    },

    "flex-layout.col#landing-pilatos-marca-banners-1-mobile":{
