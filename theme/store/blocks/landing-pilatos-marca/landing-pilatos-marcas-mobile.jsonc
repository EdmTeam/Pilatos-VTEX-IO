        "children": [
            "slider-layout#department-banner-mobile-landing-pilatos-marca"
        ],
        "props": {
            "images": [
             
                {
                    "image": "/arquivos/Pilatos-RitvalesLanding-Mobile1.jpg",
                    "description": "Promotional Banner",
                    "link": {
                        "url": "",
                        "attributeTitle": ""
                    },
                    "title": "",
                    "width": "235px"

                },
                {
                    "image": "/arquivos/Pilatos-RitvalesLanding-Mobile2.jpg",
