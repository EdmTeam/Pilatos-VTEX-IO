    "rich-text#category-morrales-hombre-bread-crumb":{
        "props":{
            "text": "[Home](/) / [**Hombre**](/hombre) / [**Accesorios**](/hombre/accesorios) / [**Morrales**](/hombre/accesorios/morrales)",
            "blockClass": "departmentBreadCrumb"
        }
    },

    "rich-text#category-morrales-hombre-title":{
        "props":{
            "text": "# Morrales para hombre",
            "blockClass": "departmentTitle"
        }
    },

    "rich-text#category-morrales-hombre-description":{
        "props":{
            "text": "Complementa tu look con los morrales para hombre que tenemos disponibles de las mejores marcas, Superdry, Diesel, Oakley, Pilatos, Replay, Quiksilver, entre otras en un solo lugar. **Morrales funcionales para hombre** en diferentes colores, estilos y diseños, ideales para cualquier momento o situación, desde ir a clase hasta ir de viaje. ¿Qué esperas para comprar tu morral?",
            "blockClass": "departmentDescription"
        }
    },

    "search-result-layout.customQuery#category-morrales-hombre": {
        "blocks": [
            "search-result-layout.desktop",
            "search-result-layout.mobile#category-morrales-hombre"
        ],
        "props": {
            "querySchema": {
                "orderByField": "OrderByReleaseDateDESC",
                "hideUnavailableItems": true,
                "maxItemsPerPage": 48,
                "queryField": "hombre/accesorios/morrales",
                "skusFilter": "ALL",
                "mapField": "category-1"
            }
        }
    },

    "flex-layout.row#category-morrales-hombre-banners-1":{
        "children":[
            "image#category-morrales-hombre-banners-1-img-1",
            "image#category-morrales-hombre-banners-1-img-2",
