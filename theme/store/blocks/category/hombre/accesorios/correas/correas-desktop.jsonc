{
    "store.custom#category-correas-hombre":{
        "blocks":[
            "flex-layout.row#banner-category-correas-hombre",
            "search-result-layout.customQuery#category-correas-hombre"
        ],
        "props":{
            "blockClass": "contentStoreCustom"
        }
    },

    "flex-layout.row#banner-category-correas-hombre":{
        "children":[
            "flex-layout.col#banner-category-correas-hombre"
        ],
        "props":{
            "blockClass": "contentSearch"
        }
    },

    "flex-layout.col#banner-category-correas-hombre":{
        "children":[
            "rich-text#category-correas-hombre-bread-crumb",
            "rich-text#category-correas-hombre-title",
            "rich-text#category-correas-hombre-description"
