{
    "store.custom#category-polos-hombre":{
        "blocks":[
            "flex-layout.row#banner-category-polos-hombre",
            "search-result-layout.customQuery#category-polos-hombre"
        ],
        "props":{
            "blockClass": "contentStoreCustom"
        }
    },

    "flex-layout.row#banner-category-polos-hombre":{
        "children":[
            "flex-layout.col#banner-category-polos-hombre"
        ],
        "props":{
            "blockClass": "contentSearch"
        }
    },

    "flex-layout.col#banner-category-polos-hombre":{
        "children":[
            "rich-text#category-polos-hombre-bread-crumb",
            "rich-text#category-polos-hombre-title",
            "rich-text#category-polos-hombre-description"
            // "responsive-layout.desktop#category-polos-hombre-banners",
            // "responsive-layout.phone#category-polos-hombre-banners",
            // "responsive-layout.tablet#category-polos-hombre-banners"
        ]
    },

    "responsive-layout.desktop#category-polos-hombre-banners":{
        "children":[
            "flex-layout.row#category-polos-hombre-banners-1"
        ]
    },

    "rich-text#category-polos-hombre-bread-crumb":{
        "props":{
            "text": "[Home](/) / [**Hombre**](/hombre) / [**Ropa**](/hombre/ropa) / [**Polos**](/hombre/ropa/polos)",
            "blockClass": "departmentBreadCrumb"
        }
    },

    "rich-text#category-polos-hombre-title":{
        "props":{
            "text": "# Polos para hombre",
            "blockClass": "departmentTitle"
        }
    },

    "rich-text#category-polos-hombre-description":{
        "props":{
            "text": "Encuentra diferentes polos para hombre de las mejores marcas: Marithe Francois Girbaud, Celio, New Project, Pilatos, Superdry, Replay, Diesel y New Balance. Descubre todos los diseños, colores y estampados en las **camisetas polo de moda** para hombre que se ajustan a cada ocasión. ",
            "blockClass": "departmentDescription"
        }
    },

    "search-result-layout.customQuery#category-polos-hombre": {
        "blocks": [
            "search-result-layout.desktop",
            "search-result-layout.mobile#category-polos-hombre"
        ],
        "props": {
            "querySchema": {
                "orderByField": "OrderByReleaseDateDESC",
                "hideUnavailableItems": true,
                "maxItemsPerPage": 48,
                "queryField": "hombre/ropa/polos",
                "skusFilter": "ALL",
                "mapField": "category-1"
            }
        }
    },

    "flex-layout.row#category-polos-hombre-banners-1":{
        "children":[
            "image#category-polos-hombre-banners-1-img-1",
            "image#category-polos-hombre-banners-1-img-2",
            "image#category-polos-hombre-banners-1-img-3"
        ],
        "props":{
            "blockClass":"contentCategoryBannersDesk"
        }
    },

    "image#category-polos-hombre-banners-1-img-1": {
        "props": {
            "src": "/arquivos/category-bn-desk-3.png",
            "link": {
                "url": "/",
                "attributeTitle": ""
            },
            "blockClass": "departmentBannerImg-1"
        }
    },

    "image#category-polos-hombre-banners-1-img-2": {
        "props": {
            "src": "/arquivos/category-bn-desk-2.png",
            "link": {
                "url": "/",
                "attributeTitle": ""
            },
            "blockClass": "departmentBannerImg-2"
        }
    },

    "image#category-polos-hombre-banners-1-img-3": {
        "props": {
            "src": "/arquivos/category-bn-desk-1.png",
            "link": {
                "url": "/",
                "attributeTitle": ""
            },
            "blockClass": "departmentBannerImg-3"
        }
    }
}