    "rich-text#category-medias-mujer-bread-crumb":{
        "props":{
            "text": "[Home](/) / [**Mujer**](/mujer) / [**Ropa Interior**](/mujer/ropa-interior) / [**Medias**](/mujer/ropa-interior/medias)",
            "blockClass": "departmentBreadCrumb"
        }
