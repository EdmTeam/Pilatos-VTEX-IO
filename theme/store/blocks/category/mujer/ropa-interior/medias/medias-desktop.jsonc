    "rich-text#category-medias-mujer-title":{
        "props":{
            "text": "Medias para mujer",
            "blockClass": "departmentTitle"
        }
    },

    "rich-text#category-medias-mujer-description":{
        "props":{
            "text": "¿Coloridas, estampadas, unicolor o de rayas? Descubre **diferentes diseños de medias para mujer**, acompañado de las mejores marcas como Stance y Superdry, perfectas para todos los estilos y ocasiones. ¡Elige tus favoritas y siente su comodidad!",
            "blockClass": "departmentDescription"
        }
    },

    "search-result-layout.customQuery#category-medias-mujer": {
        "blocks": [
            "search-result-layout.desktop",
            "search-result-layout.mobile#category-medias-mujer"
        ],
        "props": {
            "querySchema": {
                "orderByField": "OrderByScoreDESC",
                "hideUnavailableItems": true,
                "maxItemsPerPage": 48,
                "queryField": "mujer/ropa-interior/medias",
                "skusFilter": "ALL",
                "mapField": "category-1"
            }
        }
    },

    "flex-layout.row#category-medias-mujer-banners-1":{
        "children":[
            "image#category-medias-mujer-banners-1-img-1",
