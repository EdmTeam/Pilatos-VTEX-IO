{
    "responsive-layout.phone#page-nuevo-banners":{
        "children":[
            "flex-layout.col#page-nuevo-banners-1-mobile"
        ]
    },

    "flex-layout.col#page-nuevo-banners-1-mobile":{
        "children":[
            "image#page-nuevo-banners-1-img-1-mobile"
        ],
        "props":{
            "blockClass":"contentBannersDesk"
        }
    },

    "image#page-nuevo-banners-1-img-1-mobile": {
        "props": {
            "src": "/arquivos/Nuevos_Banner_principal_Mobile.jpg",
            "link": {
                "url": "/nuevo",
                "attributeTitle": ""
            },
            "blockClass": "departmentBannerImg-1"
        }
    },

    //Tablet
    "responsive-layout.tablet#page-nuevo-banners":{
        "children":[
            "flex-layout.col#page-nuevo-banners-1-tablet"
        ]
    },

    "flex-layout.col#page-nuevo-banners-1-tablet":{
        "children":[
            "image#page-nuevo-banners-1-img-1-tablet"
        ],
        "props":{
            "blockClass":"contentBannersDesk"
        }
    },

    "image#page-nuevo-banners-1-img-1-tablet": {
        "props": {
            "src": "/arquivos/Nuevos_Banner_principal_Tablet.jpg",
            "link": {
                "url": "/nuevo",
                "attributeTitle": ""
            },
            "blockClass": "departmentBannerImg-1"
        }
    }
}