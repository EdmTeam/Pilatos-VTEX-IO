        "blocks":[
            "flex-layout.row#banner-page-nuevo",
            "flex-layout.row#shelf-nuevo"
        ],
        "props":{
            "blockClass": "contentStoreCustom"
        }
    },

    "flex-layout.row#banner-page-nuevo":{
